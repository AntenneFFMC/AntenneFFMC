	<options>typo_guillemets_options.php</options>
	<pipeline>
		<nom>post_typo</nom>
		<action>remplacements</action>
		<inclure>typo_guillemets_fonctions.php</inclure>
	</pipeline>
	<pipeline>
		<nom>insert_head_css</nom>
		<inclure>typo_guillemets_fonctions.php</inclure>
	</pipeline>
	
